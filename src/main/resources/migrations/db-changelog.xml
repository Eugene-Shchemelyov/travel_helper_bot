<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="1558423630" author="developer">
        <createTable tableName="T_CITY">
            <column name="F_ID" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="F_NAME" type="VARCHAR(255)">
                <constraints unique="true" nullable="false"/>
            </column>
            <column name="F_INFO" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1558423640" author="developer">
        <addAutoIncrement catalogName="travel"
                          columnDataType="BIGINT"
                          columnName="F_ID"
                          incrementBy="1"
                          schemaName="public"
                          startWith="1"
                          tableName="T_CITY"/>
    </changeSet>
</databaseChangeLog>